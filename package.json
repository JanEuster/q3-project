{
  "name": "expanded-board",
  "description": "open source drawing application on the web",
  "author": "Jan 'JanEuster' Eusterschulte <jan.eusterschulte@rhgy.de>",
  "contributors": [
    "Lennart Brunn <lennart.brunn@rhgy.de>",
    "Friedrich Maagk <friedrich.maagk@rhgy.de>"
  ],
  "version": "0.2.0",
  "homepage": ".",
  "private": true,
  "build": {
    "remoteBuild": false,
    "icon": "build/icon.png",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "q3-project",
      "owner": "janeuster"
    }
  },
  "dependencies": {
    "@testing-library/user-event": "^13.2.1",
    "chokidar": "^3.5.2",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "npm": "^8.3.0",
    "react": "^17.0.2",
    "react-dev-utils": "^12.0.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.0",
    "styled-components": "^5.3.3",
    "web-vitals": "^2.1.0",
    "xml-formatter": "^2.6.0"
  },
  "overrides": {
    "react-scripts": {
      "@svgr/webpack": "^6.1.2"
    }
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "build-je": "react-scripts build && cp build/* janeuster.github.io/ -r",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "pack-win": "electron-builder build --win=portable",
    "pack-linux": "electron-builder build --linux"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "concurrently": "^6.2.1",
    "electron": "^12.2.3",
    "electron-builder": "^22.14.13",
    "gh-pages": "^3.2.3",
    "react-git-info": "^2.0.1",
    "wait-on": "^6.0.0"
  }
}